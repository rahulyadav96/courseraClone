<script>

let parent = document.getElementById("main-course-Cont");

let course = parent.querySelectorAll('.course-cont');

function filterElement(){
    let option = document.getElementById("level-option").value;

    let course = parent.querySelectorAll('.course-cont');

    for(let i = 0; i< course.length; i++){
            
        let crs_level = course[i].querySelector(".crs-level").innerHTML;
           
        course[i].style.display = "contents";
        if(crs_level != option){
            course[i].style.display = "none";
            }
    }
}



</script>